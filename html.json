		"For Loop": {
			"prefix": "for",
			"body": [
				"<?php for ($$i = ${1}; $$i ${2}; $$i${3}): ?>",
				"\t${4}",
				"<?php endfor; ?>"
			],
			"description": "This is a for loop made with php. Made by: Toni B. Lopez."
